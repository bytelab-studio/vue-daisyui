{
    "name": "vue-daisyui",
    "private": true,
    "version": "0.0.0",
    "main": "dist/vue-daisyui.umd.js",
    "module": "dist/vue-daisyui.es.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc -b && vite build",
        "preview": "vite preview",
        "docs:tailwindcss": "npx @tailwindcss/cli -i docs/.vitepress/theme/style.css -o docs/public/tailwindcss.bundle.css --watch",
        "docs:dev": "vitepress dev docs",
        "docs:build": "npx @tailwindcss/cli -i docs/.vitepress/theme/style.css -o docs/public/tailwindcss.bundle.css -m --optimize && vitepress build docs",
        "docs:preview": "vitepress preview docs"
    },
    "peerDependencies": {
        "daisyui": "^5.1.27",
        "tailwindcss": "^4.1.14",
        "vue": "^3.5.22"
    },
    "devDependencies": {
        "@tailwindcss/cli": "^4.1.14",
        "@types/node": "^24.6.0",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/tsconfig": "^0.8.1",
        "typescript": "~5.9.3",
        "vite": "^7.1.7",
        "vite-plugin-dts": "^4.5.4",
        "vitepress": "^1.6.4",
        "vue-shadow-dom": "^4.2.0",
        "vue-tsc": "^3.1.0"
    }
}
